# Render API Key (required for all services)
# Get this from your Render dashboard
RENDER_API_KEY=your_render_api_key_here

# OpenAI API Key (optional, required for OpenAI examples)
# Get this from https://platform.openai.com/api-keys
OPENAI_API_KEY=your_openai_api_key_here

# Backend URL (for frontend in production)
# Point this to your deployed backend service URL
VITE_API_URL=https://your-backend.onrender.com
